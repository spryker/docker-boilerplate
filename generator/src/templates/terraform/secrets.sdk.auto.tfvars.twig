spryker_secrets = {
{% for secretKey, secretValue in project['secrets'] %}
    {{ secretKey | raw }} = {{ secretValue | tf_var }}
{% endfor %}
    SPRYKER_PAAS_SERVICES = {{ '{"version": "1.0", "databases": []}' | tf_var }}
    SPRYKER_FEATURES = {{ '{}' | tf_var }}
    SPRYKER_MAINTENANCE_MODE_ENABLED = {{ 0 | tf_var }}
}
