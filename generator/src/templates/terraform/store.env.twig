{% if project['dynamicStoreMode'] %}
export SPRYKER_CURRENT_REGION={{ regionName | env_var }}
{% else %}
export APPLICATION_STORE={{ project['_storeSpecific'][storeName]['APPLICATION_STORE'] | env_var }}
export SPRYKER_KEY_VALUE_REGION_NAMESPACES={{ project['_storeSpecific'][regionName]['SPRYKER_KEY_VALUE_REGION_NAMESPACES'] | env_var }}
export SPRYKER_ACTIVE_STORES={{ project['regions'][regionName]['stores'] | keys | join(',') | env_var }}
{% endif %}
export SPRYKER_SEARCH_NAMESPACE={{ project['_storeSpecific'][identifier]['SPRYKER_SEARCH_NAMESPACE'] | env_var }}
export SPRYKER_KEY_VALUE_STORE_NAMESPACE={{ project['_storeSpecific'][identifier]['SPRYKER_KEY_VALUE_STORE_NAMESPACE'] | env_var }}
export SPRYKER_BROKER_NAMESPACE={{ project['_storeSpecific'][identifier]['SPRYKER_BROKER_NAMESPACE'] | env_var }}
export SPRYKER_SESSION_BE_NAMESPACE={{ project['_storeSpecific'][identifier]['SPRYKER_SESSION_BE_NAMESPACE'] | env_var }}
export SPRYKER_FE_HOST={{ endpointMap[identifier]['yves'] | default('')  | split(':') | first | env_var }}
export SPRYKER_API_HOST={{ endpointMap[identifier]['glue'] | default('') | split(':') | first | env_var }}
export SPRYKER_BE_HOST={{ endpointMap[identifier]['backoffice'] | default('')  | split(':') | first | env_var }}
export SPRYKER_MP_HOST={{ endpointMap[storeName]['merchant-portal'] | default('')  | split(':') | first | env_var }}
export SPRYKER_SSL_ENABLE="{{ project['docker']['ssl']['enabled'] ? 1 : 0}}"
