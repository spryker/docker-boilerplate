{% extends "nginx/application.server.conf.twig" %}
{% block server %}
{{ parent() }}

  include /etc/nginx/vhost.d/yves.default.conf;
{% endblock server %}
{% block location %}
{{ parent() }}

    fastcgi_param SPRYKER_SESSION_FE_NAMESPACE "{{ storeServices['session']['namespace'] | default(domainData['services']['session']['namespace']) }}";
    fastcgi_param SPRYKER_FE_HOST {{ host }};
    fastcgi_param SPRYKER_FE_PORT {{ port }};

    root /data/public/Yves;
{% endblock location %}
