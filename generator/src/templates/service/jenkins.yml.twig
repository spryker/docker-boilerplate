  {{ serviceName }}:
    image: spryker/jenkins:2.176
    networks:
      - services
      - private
    healthcheck:
      test: [ "CMD", "nc", "-z", "localhost", "8080" ]
      interval: 10s
      timeout: 5s
      retries: 5
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock:ro
      - {{ serviceName }}-data:/var/jenkins_home:rw
