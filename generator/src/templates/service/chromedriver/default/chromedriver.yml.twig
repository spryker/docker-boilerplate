  {{ serviceName }}:
    image: zyuzka/chromedriver:latest
    networks:
      - public
    init: true
    labels:
      'spryker.app.name': webdriver
      'spryker.app.type': services
      'spryker.project': ${SPRYKER_DOCKER_PREFIX}:${SPRYKER_DOCKER_TAG}
    expose:
      - "4444"
    healthcheck:
      test: [ "CMD", "nc", "-z", "localhost", "4444" ]
      interval: 5s
      timeout: 5s
      retries: 5
    command: [ "--port=4444", "--whitelisted-ips=", "--url-base=/wd/hub", "--allowed-origins=*" ]
