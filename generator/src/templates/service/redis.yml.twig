  {{ serviceName }}:
    image: redis:5.0-alpine
    command: 'redis-server --appendonly yes --save \"\"'
    networks:
      - services
      - private
    healthcheck:
        test: [ "CMD", "redis-cli", "ping" ]
        interval: 10s
        timeout: 5s
        retries: 5
    volumes:
      - {{ serviceName }}-{{ serviceData['engine'] }}-data:/data
