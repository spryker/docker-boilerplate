  {{ serviceData.project_name ~ '_' ~ serviceName }}:
    build:
      context: {{ serviceData.deployment_path }}
      dockerfile: images/common/dashboard/Dockerfile
    container_name: {{ serviceData.project_name ~ '_' ~ serviceName }}
    networks:
      - {{ internal_project_name }}_public
      - {{ internal_project_name }}_private
    labels:
      'spryker.app.name': {{ serviceData.project_name ~ '_' ~ serviceName }}
      'spryker.app.type': hidden
      'spryker.project': ${SPRYKER_DOCKER_PREFIX}:${SPRYKER_DOCKER_TAG}
    environment:
      PROJECT: ${SPRYKER_DOCKER_PREFIX}:${SPRYKER_DOCKER_TAG}
      SPRYKER_LOG_DIRECTORY: ${SPRYKER_LOG_DIRECTORY}
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
      - {{ serviceData.deployment_path }}/context/dashboard/environment:/dashboard/environment:consistent,ro
    privileged: true

